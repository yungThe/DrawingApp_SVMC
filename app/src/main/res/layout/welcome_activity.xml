<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="428dp"
        android:layout_height="408dp"
        app:srcCompat="@drawable/logoDraw"
        tools:layout_editor_absoluteX="-9dp"
        tools:layout_editor_absoluteY="-45dp" />

    <ImageButton
        android:id="@+id/btnNew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_baseline_play_arrow_24"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="424dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="126dp"
        android:layout_height="20dp"
        android:text="New project"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="435dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_baseline_image_24"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="498dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Open project"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="509dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_baseline_contact_support_24"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="733dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Need support?"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="792dp" />

</androidx.constraintlayout.widget.ConstraintLayout>